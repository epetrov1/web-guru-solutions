#: env/lib/python3.9/site-packages/django/contrib/flatpages/admin.py:13
msgid "Advanced options"
msgstr "Допълнителни опции"

#: env/lib/python3.9/site-packages/django/contrib/flatpages/apps.py:8
msgid "Flat Pages"
msgstr "Информативни страници"

#: env/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:11
#: env/lib/python3.9/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: env/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:15
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Пример: \"/about/contact/\".  Началната и крайната наклонена чертичка са "
"задължителни."

#: env/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:20
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Тази стойност трябва да съдържа само букви, цифри, точки, долни тирета, "
"тирета, наклонени черти или тилди."

#: env/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:34
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr ""
"Пример: \"/about/contact/\". Началната наклонена чертичка е задължителна. "

#: env/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:47
msgid "URL is missing a leading slash."
msgstr "В URL липсва начална наклонена черта."

#: env/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:52
msgid "URL is missing a trailing slash."
msgstr "В URL липсва завършваща наклонена черта."

#: env/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:69
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "Flatpage с url %(url)s вече съществува за сайт %(site)s"

#: env/lib/python3.9/site-packages/django/contrib/flatpages/models.py:10
msgid "title"
msgstr "заглавие"

#: env/lib/python3.9/site-packages/django/contrib/flatpages/models.py:11
msgid "content"
msgstr "съдържание"

#: env/lib/python3.9/site-packages/django/contrib/flatpages/models.py:12
msgid "enable comments"
msgstr "позволяване на коментари"

#: env/lib/python3.9/site-packages/django/contrib/flatpages/models.py:14
msgid "template name"
msgstr "име на шаблон"

#: env/lib/python3.9/site-packages/django/contrib/flatpages/models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Пример: \"flatpages/contact_page.html\".  Ако това не е указано, системата "
"ще използва \"flatpages/default.html\".  "

#: env/lib/python3.9/site-packages/django/contrib/flatpages/models.py:23
msgid "registration required"
msgstr "изисква се регистрация"

#: env/lib/python3.9/site-packages/django/contrib/flatpages/models.py:25
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Ако това е чекнато, само логнати потребители ще могат да виждат страницата.  "

#: env/lib/python3.9/site-packages/django/contrib/flatpages/models.py:29
msgid "sites"
msgstr "сайтове"

#: env/lib/python3.9/site-packages/django/contrib/flatpages/models.py:33
msgid "flat page"
msgstr "информативна страница"

#: env/lib/python3.9/site-packages/django/contrib/flatpages/models.py:34
msgid "flat pages"
msgstr "информативни страници"
