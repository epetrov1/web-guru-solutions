#: env/lib/python3.9/site-packages/django/contrib/postgres/apps.py:54
msgid "PostgreSQL extensions"
msgstr "PostgreSQL разширения"

#: env/lib/python3.9/site-packages/django/contrib/postgres/fields/array.py:21
#: env/lib/python3.9/site-packages/django/contrib/postgres/forms/array.py:17
#: env/lib/python3.9/site-packages/django/contrib/postgres/forms/array.py:185
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Елемент %(nth)s в масива не се валидира:"

#: env/lib/python3.9/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "Вложените масиви трябва да имат еднаква дължина."

#: env/lib/python3.9/site-packages/django/contrib/postgres/fields/hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "Мап от стрингове към стрингове/null-ове"

#: env/lib/python3.9/site-packages/django/contrib/postgres/fields/hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "Стойността на “%(key)s” не е стринг или null."

#: env/lib/python3.9/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Could not load JSON data."
msgstr "Не можа да зареди JSON данни."

#: env/lib/python3.9/site-packages/django/contrib/postgres/forms/hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr "Входните данни трябва да са JSON речник."

#: env/lib/python3.9/site-packages/django/contrib/postgres/forms/ranges.py:42
msgid "Enter two valid values."
msgstr "Въведете две валидни стойности."

#: env/lib/python3.9/site-packages/django/contrib/postgres/forms/ranges.py:44
msgid "The start of the range must not exceed the end of the range."
msgstr "Началото на обхвата не трябва да превишава края му."

#: env/lib/python3.9/site-packages/django/contrib/postgres/forms/ranges.py:99
msgid "Enter two whole numbers."
msgstr "Въведете две цели числа."

#: env/lib/python3.9/site-packages/django/contrib/postgres/forms/ranges.py:105
msgid "Enter two numbers."
msgstr "Въведете две числа."

#: env/lib/python3.9/site-packages/django/contrib/postgres/forms/ranges.py:111
msgid "Enter two valid date/times."
msgstr "Въведете две валидни дати/времена."

#: env/lib/python3.9/site-packages/django/contrib/postgres/forms/ranges.py:117
msgid "Enter two valid dates."
msgstr "Въведете две коректни дати."

#: env/lib/python3.9/site-packages/django/contrib/postgres/validators.py:15
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Списъкът съдържа %(show_value)d елемент, а трябва да има не повече от "
"%(limit_value)d."
msgstr[1] ""
"Списъкът съдържа %(show_value)d елемента, а трябва да има не повече от "
"%(limit_value)d."

#: env/lib/python3.9/site-packages/django/contrib/postgres/validators.py:25
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Списъкът съдържа %(show_value)d елемент, а трябва да има поне "
"%(limit_value)d."
msgstr[1] ""
"Списъкът съдържа %(show_value)d елемента, а трябва да има поне "
"%(limit_value)d."

#: env/lib/python3.9/site-packages/django/contrib/postgres/validators.py:38
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Някои ключове липсват: %(keys)s"

#: env/lib/python3.9/site-packages/django/contrib/postgres/validators.py:39
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Бяха предоставени някои неизвестни ключове: %(keys)s"

#: env/lib/python3.9/site-packages/django/contrib/postgres/validators.py:81
#, fuzzy, python-format
#| msgid ""
#| "Ensure that this range is completely greater than or equal to "
#| "%(limit_value)s."
msgid ""
"Ensure that the upper bound of the range is not greater than %(limit_value)s."
msgstr ""
"Уверете се, че интервалът е изцяло по-голям от или равен на %(limit_value)s."

#: env/lib/python3.9/site-packages/django/contrib/postgres/validators.py:90
#, fuzzy, python-format
#| msgid ""
#| "Ensure that this range is completely less than or equal to "
#| "%(limit_value)s."
msgid ""
"Ensure that the lower bound of the range is not less than %(limit_value)s."
msgstr ""
"Уверете се, че този обхват е изцяло по-малък от или равен на %(limit_value)s."
